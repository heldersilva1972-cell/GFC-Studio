# Manually set the theme without the buggy init script
$env:POSH_THEMES_PATH = "$env:LOCALAPPDATA\Programs\oh-my-posh\themes"

function prompt {
    # This calls the tool ONLY for the text, skipping all keyboard hooks
    & oh-my-posh print primary --config 'https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/jandedobbeleer.omp.json'
}